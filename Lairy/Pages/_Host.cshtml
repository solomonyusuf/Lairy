@page "/"
@namespace Lairy.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Lairy</title>
    <base href="~/" />
    <link rel="stylesheet" href="css/bootstrap/bootstrap.min.css" />
    <link href="css/site.css" rel="stylesheet" />
    <link href="Lairy.styles.css" rel="stylesheet" />
</head>
<body>
    <component type="typeof(App)" render-mode="ServerPrerendered" />

    <div id="blazor-error-ui">
        <environment include="Staging,Production">
            An error has occurred. This application may no longer respond until reloaded.
        </environment>
        <environment include="Development">
            An unhandled exception has occurred. See browser dev tools for details.
        </environment>
        <a href="" class="reload">Reload</a>
        <a class="dismiss">🗙</a>
    </div>

 <script src="_framework/blazor.server.js"></script>
    <!-- Jquery Core Js --> 
<script src="assets/bundles/libscripts.bundle.js"></script> <!-- Lib Scripts Plugin Js ( jquery.v3.2.1, Bootstrap4 js) --> 
<script src="assets/bundles/vendorscripts.bundle.js"></script> <!-- slimscroll, waves Scripts Plugin Js -->

<script src="assets/bundles/jvectormap.bundle.js"></script> <!-- JVectorMap Plugin Js -->
<script src="assets/bundles/sparkline.bundle.js"></script> <!-- Sparkline Plugin Js -->
<script src="assets/bundles/c3.bundle.js"></script>

<script src="assets/bundles/mainscripts.bundle.js"></script>
<script src="assets/js/pages/index.js"></script>

<script src="js/vendor/modernizr-3.6.0.min.js"></script>
    <script src="js/vendor/jquery-1.12.4.min.js"></script>

    <!--====== Bootstrap js ======-->
    <script src="admin/js/bootstrap.min.js"></script>
    
    <!--====== Slick js ======-->
    <script src="admin/js/slick.min.js"></script>
    
    <!--====== Magnific Popup js ======-->
    <script src="admin/js/jquery.magnific-popup.min.js"></script>
    
    <!--====== Counter Up js ======-->
    <script src="admin/js/waypoints.min.js"></script>
    <script src="admin/js/jquery.counterup.min.js"></script>
    
    <!--====== Nice Select js ======-->
    <script src="admin/js/jquery.nice-select.min.js"></script>
    
    <!--====== Nice Number js ======-->
    <script src="admin/js/jquery.nice-number.min.js"></script>
    
    <!--====== Count Down js ======-->
    <script src="admin/js/jquery.countdown.min.js"></script>
    
    <!--====== Validator js ======-->
    <script src="admin/js/validator.min.js"></script>
    
    <!--====== Ajax Contact js ======-->
    <script src="admin/js/ajax-contact.js"></script>
    
    <!--====== Main js ======-->
    <script src="admin/js/main.js"></script>
    
    <!--====== Map js ======-->
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDC3Ip9iVC0nIxC6V14CKLQ1HZNF_65qEQ"></script>
    <script src="admin/js/map-script.js"></script>
</body>
</html>
